{
  "name": "explo-code",
  "publisher": "blaugold",
  "displayName": "Explo",
  "description": "Explore the render tree of a Flutter app in 3D, through an exploded representation.",
  "repository": {
    "url": "https://github.com/blaugold/explo",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/blaugold/explo/issues"
  },
  "author": {
    "name": "Gabriel Terwesten",
    "email": "gabriel@terwesten.net"
  },
  "license": "FOR LICENSE SEE LICENSE",
  "version": "0.0.0",
  "engines": {
    "vscode": "^1.63.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "explo.openView",
        "title": "Open view",
        "category": "Explo",
        "enablement": "inDebugMode && debugType == 'dart'"
      }
    ]
  },
  "extensionDependencies": [
    "Dart-Code.dart-code"
  ],
  "scripts": {
    "vscode:prepublish": "melos run explo_ide_view:vs_code &&  npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.63.0",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "@vscode/test-electron": "^1.6.2",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "glob": "^7.1.7",
    "mocha": "^9.1.3",
    "ts-loader": "^9.2.5",
    "typescript": "^4.4.4",
    "webpack": "^5.52.1",
    "webpack-cli": "^4.8.0"
  }
}
